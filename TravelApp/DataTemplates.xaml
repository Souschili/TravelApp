<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TravelApp"
                    xmlns:v="clr-namespace:TravelApp.Views"
                    xmlns:vm="clr-namespace:TravelApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:SignUpPageViewModel}">
        <v:SignUpPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LogInPageViewModel}">
        <v:LogInPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainPageViewModel}">
        <v:MainPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:NewTripPageViewModel}">
            <v:NewTripPageView/>
     </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ShowPdfPageViewModel}">
        <v:ShowPdfPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ShowMapPageViewModel}">
        <v:ShowMapPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ShowGoogleMapPageViewModel}">
        <v:ShowGoogleMapPageView/>
    </DataTemplate>

</ResourceDictionary>